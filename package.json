{
	"name": "kitegg-directus-keycloak-endpoint",
	"description": "Keycloak user administration through Directus",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-endpoint"
	],
	"type": "module",
	"directus:extension": {
		"type": "endpoint",
		"path": "src/index.js",
		"source": "src/index.js",
		"host": "^10.1.5"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.1.5"
	},
	"dependencies": {
		"@directus/errors": "^0.0.1",
		"axios": "^1.4.0",
		"querystring": "^0.2.1",
		"secure-random-password": "^0.2.3",
		"validator": "^13.9.0"
	}
